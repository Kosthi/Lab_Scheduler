            <!--            <HBox.margin>-->
            <!--                <Insets left="2.0" right="4.0"/>-->
            <!--            </HBox.margin>-->
        </MFXComboBox>
        <MFXButton contentDisplay="GRAPHIC_ONLY" depthLevel="LEVEL0" ellipsisString=""
